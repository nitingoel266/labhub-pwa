{
  "name": "labhub_pwa",
  "version": "0.3.19",
  "private": true,
  "scripts": {
    "dev": "BROWSER=none REACT_APP_ENV=dev react-scripts start",
    "start": "node server.js",
    "build": "npm run clean && npm ci && REACT_APP_ENV=prod react-scripts build",
    "prod": "npm run build && npm run pm2:stop && npm run pm2:start",
    "pm2:start": "pm2 start server.js --name labhub-pwa --time",
    "pm2:stop": "pm2 delete labhub-pwa || true",
    "pm2:logs": "pm2 logs labhub-pwa",
    "pm2:ls": "pm2 ls",
    "clean": "rm -rf build node_modules",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "async-sema": "^3.1.1",
    "chart.js": "^4.2.1",
    "chartjs-plugin-zoom": "^2.0.0",
    "express": "^4.18.2",
    "fastq": "^1.15.0",
    "pm2": "^5.3.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.8.1",
    "react-simple-wheel-picker": "^1.2.0",
    "rxjs": "^7.8.0",
    "short-uuid": "^4.2.2",
    "socket.io-client": "^4.6.1"
  },
  "devDependencies": {
    "@types/node": "17.0.45",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/web-bluetooth": "^0.0.16",
    "react-scripts": "5.0.1",
    "typescript": "4.9.5",
    "workbox-background-sync": "6.5.4",
    "workbox-broadcast-update": "6.5.4",
    "workbox-cacheable-response": "6.5.4",
    "workbox-core": "6.5.4",
    "workbox-expiration": "6.5.4",
    "workbox-google-analytics": "6.5.4",
    "workbox-navigation-preload": "6.5.4",
    "workbox-precaching": "6.5.4",
    "workbox-range-requests": "6.5.4",
    "workbox-routing": "6.5.4",
    "workbox-strategies": "6.5.4",
    "workbox-streams": "6.5.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
